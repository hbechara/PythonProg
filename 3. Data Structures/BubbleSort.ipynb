      "source": [
        "# Bubble sort\n",
        "\n",
        "nlist = [14,46,43,27,57,41,45,21,70] # here is our list of random numbers\n",
        "for passnum in range(len(nlist)-1,0,-1): #\n",
        " for i in range(passnum):\n",
        "   if nlist[i]>nlist[i+1]:\n",
        "     temp = nlist[i]\n",
        "     nlist[i] = nlist[i+1]\n",
        "     nlist[i+1] = temp\n",
        "print(nlist)"
      ],
